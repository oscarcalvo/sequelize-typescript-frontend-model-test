{
    "compilerOptions": {
        "declaration": false,
        "importHelpers": true,
        "jsx": "react",
        "lib": ["dom", "es2017"],
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "target": "es6",
        "sourceMap": true,
        "removeComments": true,
        "noStrictGenericChecks": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true
    }
}
